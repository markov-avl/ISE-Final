{
  "trailingComma": "none",
  "useTabs": true,
  "semi": false,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/types/(.*)$",
    "^@/assets/(.*)$",
    "^@/utils/(.*)$",
    "^@/hooks/(.*)$",
    "^@/store/(.*)$",
    "^@/shared/(.*)$",
    "^@/components/(.*)$",
    "^./(.*)$",
    "(.css)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}